{
  "name": "nextjs_boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "email": "email dev --dir emails --port 8081",
    "prisma:studio": "prisma studio",
    "prisma:generate": "prisma generate",
    "prisma:db_push": "prisma db push",
    "prisma:migrate_dev": "prisma migrate dev",
    "prisma:migrate_deploy": "prisma migrate deploy",
    "dev": "concurrently --kill-others \"npm run prisma:generate && next dev --port 3000\"",
    "dev:all": "concurrently --kill-others \"npm run prisma:generate && next dev --port 3000\" \"npm run prisma:studio\" \"npm run email --force\"",
    "build": "npm run prisma:generate && next build",
    "lint": "next lint",
    "start": "next start"
  },
